{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true,
          "experimentalObjectRestSpread": true
      }
  },
  "globals": {
    "it": true,
    "assert": false
  },
  "rules": {
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "no-multi-str": 0,
    "space-before-function-paren": [2, "never"],
    "space-before-blocks": [2, "always"],
    "no-debugger":0
  }
}
